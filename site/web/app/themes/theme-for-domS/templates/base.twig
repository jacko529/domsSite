{% block html_head_container %}

{% include 'html-header.twig' %}
	{% block head %}
	{% endblock %}
	</head>
{% endblock %}

	<body class="{{body_class}} container" data-template="base.twig">
		<header class="header" >
			{% block header %}
				<nav class="navbar navbar-expand-md navbar-dark  " style="margin-bottom: 100px">
					
					<h1 class="hdr-logo" role="banner">
						<a class="navbar-brand " href="{{site.url}}" rel="home">Pysch & Strength</a>
					</h1>
					
					<div class="navbar-collapse collapse justify-content-between align-items-center w-100" id="collapsingNavbar2">
							{% include "menu.twig" with {'menu': menu.get_items} %}
							
                                <a class="navbar-text blackColour" href="{{site.blog}}">Get In Touch +</a>

					</div><!-- #nav -->
					<button class="navbar-toggler" type="button" data-toggle="collapse" data-target="#collapsingNavbar">
							<span class="navbar-toggler-icon "></span>
						</button>
				</nav>
			{% endblock %}
		</header>
		<!-- model pop u -->
		
<div id="myModal" class="modal fade" role="dialog">
  <div class="modal-dialog">

    <!-- Modal content-->
    <div class="modal-content">
      <div class="modal-header">
        <button type="button" class="close" data-dismiss="modal">&times;</button>
        <h4 class="modal-title">Modal Header</h4>
      </div>
      <div class="modal-body">
        <p>Some text in the modal.</p>
      </div>
      <div class="modal-footer">
        <div class="input-group">
          <input type="checkbox" id="remember"> Do not show again
        </div>
        <button type="button" class="btn btn-default" data-dismiss="modal">Close</button>
      </div>
    </div>

  </div>
</div>
		<section id="content" role="main" class="content-wrapper container">
			{% if title %}<h1>{{title}}</h1>{% endif %}
			<div class="wrapper {{sidebar_class}}">
				{% block content %}
					Sorry, no content
				{% endblock %}
			</div>
			{% if sidebar %}
				<aside class="layout-sidebar">
					{{sidebar}}
				</aside>
			{% endif %}
		</section>

		{% block footer %}
			<footer id="footer">
				{% include 'footer.twig' %}
			</footer>
			{{ function('wp_footer') }}
		{% endblock %}
	</body>
</html>
